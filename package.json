{
  "name": "superset-plugin-chart-ymaps",
  "version": "0.1.4",
  "description": "Superset plugin Yandex Maps",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "esm",
    "lib"
  ],
  
  "scripts": {
    "build": "npm run build-cjs && npm run build-esm && npm run ts-types",
    "build-cjs": "babel src --extensions \".ts,.tsx,.js,.jsx\" --copy-files --out-dir lib",
    "build-clean": "npm run clean && npm run build",
    "build-esm": "BABEL_OUTPUT=esm babel src --extensions \".ts,.tsx,.js,.jsx\" --copy-files --out-dir esm",
    "dev": "BABEL_OUTPUT=esm babel src --extensions \".ts,.tsx,.js,.jsx\" --watch --copy-files --out-dir esm",
    "prebuild": "rimraf {lib,esm,tsconfig.tsbuildinfo}",
    "postbuild": "npm run test",
    "ts-types": "tsc --build",
    "test": "jest"
  },
  "author": "virtus107",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@airbnb/config-babel": "^2.2.4",
    "@syncfusion/ej2-base": "^28.2.3",
    "@syncfusion/ej2-react-pivotview": "^28.2.3",
    "@turf/turf": "^7.2.0"
  },
  "peerDependencies": {
    "@airbnb/config-babel": "^2.0.1",
    "@superset-ui/chart-controls": "*",
    "@superset-ui/core": "^0.20.2",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@types/jest": "^26.0.4",
    "@superset-ui/core": "^0.20.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  },
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virtusjit/superset-plugin-chart-ymaps.git"
  },
  "bugs": {
    "url": "https://github.com/virtusjit/superset-plugin-chart-ymaps/issues"
  },
  "homepage": "https://github.com/virtusjit/superset-plugin-chart-ymaps#readme"
}
